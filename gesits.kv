<PilLayout@BoxLayout>
    size_hint: .1, .04
    padding:"7dp"
    canvas:
        Color:
            rgba:(146/255,218/255,241/255,1)
        RoundedRectangle:
            size: self.size
            pos: self.pos

<RoundCard@MDCard>
    md_bg_color:23/255,28/255,35/255,1
    spacing:"1dp"
    padding:"5dp"
    size_hint:1,0.2
    border_radius:50
    radius:[5]

<RoundCard_data@MDCard>
    orientation: "horizontal"
    spacing:"30dp"
    padding:"30dp"
    size_hint:0.7,1
    size: "300dp", "120dp"
    md_bg_color: 255/255,255/255,255/255,1
    border_radius:50
    radius:[5]

<Progress>
    MDDialog:
        title:'Tersambung'
        radius:[7, 7, 7, 7],
        md_bg_color:244/255,67/255,54/255,1
        size_hint:None, None
        size:400, 400

